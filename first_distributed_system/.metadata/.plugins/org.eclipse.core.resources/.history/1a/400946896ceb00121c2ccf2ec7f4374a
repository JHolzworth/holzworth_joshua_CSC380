package Controller;

import java.util.ArrayList;

public class MathLogic{

	public int add(int a, int b) {
		int sum = a+b;
		return sum;
	}

	public int subtract(int a, int b) {
		int difference = a-b;
		return difference;
	}
	
	public String TestMethod(String check){
		System.out.println(check);
		return "Test working\n"+check;
	}
}
