package UserFacingPackage;

public class FakeObject {

	byte b;
	char c;
	int i;
	double d;
	String s;
	public FakeObject(byte _b, char _c, int _i, double _d, String _s){
		b = _b;
		c = _c;
		i = _i;
		d = _d;
		s = _s;
	}
	public String FreakOUT(FakeObject fo){
		return fo.b+" "+(char)(fo.c+1)+" "+(fo.i+fo.d)+" "+fo.s.toUpperCase();
	}
}
